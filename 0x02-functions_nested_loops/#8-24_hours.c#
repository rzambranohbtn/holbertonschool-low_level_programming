#include "holberton.h"
/**
 *jack_bauer - soundtrack time
 */

void jack_bauer(void)
{
	int hours, minutes, i, j, k, l;

	for (hours = 0; hours < 24; hours++)
	{
		i = hours / 10;
		j = hours % 10;
		for (minutes = 0; minutes < 60; minutes++)
		{
			k = minutes / 10;
			l = minutes % 10;
			_putchar('0' + i);
			_putchar('0' + j);
			_putchar(':');
			_putchar('0' + k);
			_putchar('0' + l);
			_putchar('\n');
		}
	}
}
